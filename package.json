{
  "name": "chatsounds-web-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "discord-embed-proxy": "./dist-node/discord-embed-proxy.js"
  },
  "scripts": {
    "start": "cd web && ts-node-esm discord-embed-proxy.ts",
    "dev": "npm run build:js dev",
    "build": "cd web && concurrently --raw \"cross-env NODE_ENV=production npm run build:js\" \"cross-env NODE_ENV=production npm run build:node\"",
    "build:js": "rimraf dist && cd web && ts-node-esm build.ts",
    "build:node": "rimraf dist-node && cd web && ts-node-esm build-node.ts",
    "build:wasm": "cross-env RUST_LOG=info wasm-pack -vvvv build --target web",
    "typecheck": "cd web && tsc --noEmit",
    "format": "cd web && prettier --write --ignore-path ../.gitignore .",
    "lint": "cd web && concurrently --raw \"prettier --check --ignore-path ../.gitignore .\" \"eslint --ignore-path ../.gitignore .\""
  },
  "resolutions": {
    "chatsounds-web": "portal:pkg"
  },
  "dependencies": {
    "@types/async-lock": "^1.4.0",
    "@types/concat-stream": "^2.0.0",
    "@types/express": "^4.17.18",
    "@types/fluent-ffmpeg": "^2.1.22",
    "@types/memoizee": "^0.4.9",
    "@types/node": "^18.18.6",
    "async-lock": "^1.4.0",
    "concat-stream": "^2.0.0",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "memoizee": "^0.4.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@blueprintjs/core": "^4.20.2",
    "@blueprintjs/eslint-plugin": "^2.9.1",
    "@blueprintjs/icons": "^4.16.0",
    "@blueprintjs/popover2": "^1.14.11",
    "@blueprintjs/select": "^4.9.24",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/can-autoplay": "^3.0.2",
    "@types/eslint": "^8.44.3",
    "@types/glob": "^8.1.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^17.0.69",
    "@types/react-dom": "^17.0.22",
    "@types/react-refresh": "^0.14.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/eslint-plugin-tslint": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "can-autoplay": "^3.0.2",
    "chatsounds-web": "./pkg",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "esbuild": "^0.19.4",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "^8.0.1",
    "glob": "^10.3.10",
    "html-webpack-plugin": "^5.5.3",
    "lodash.debounce": "^4.0.8",
    "normalize.css": "^8.0.1",
    "prettier": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.9",
    "react-use": "^17.4.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslint": "^6.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
