{
  "name": "chatsounds-web-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "dev": "cd web && yarn run webpack serve",
    "build": "cd web && cross-env NODE_ENV=production yarn run build:js",
    "build:dev": "cd web && yarn run build:js",
    "build:js": "rimraf dist && cd web && yarn run webpack --config webpack.config.ts",
    "build:wasm": "wasm-pack build --target web",
    "webpack": "cd web && cross-env NODE_OPTIONS=\"--no-warnings --loader ts-node/esm\" webpack",
    "typecheck": "cd web && tsc --noEmit",
    "format": "cd web && prettier --write --ignore-path ../.gitignore .",
    "lint": "cd web && concurrently --raw \"prettier --check --ignore-path ../.gitignore .\" \"eslint .\""
  },
  "dependencies": {
    "chatsounds-web": "./pkg",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "execa": "^6.1.0",
    "glob": "^8.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^18.7.2",
    "copy-webpack-plugin": "^11.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "resolutions": {
    "chatsounds-web": "portal:pkg"
  }
}
