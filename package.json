{
  "name": "chatsounds-web-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "dev": "yarn run build:js dev",
    "build": "cd web && cross-env NODE_ENV=production yarn run build:js",
    "build:dev": "cd web && yarn run build:js",
    "build:js": "rimraf dist && cd web && yarn run ts-node-esm build.ts",
    "build:wasm": "wasm-pack build --target web",
    "typecheck": "cd web && tsc --noEmit",
    "format": "cd web && prettier --write --ignore-path ../.gitignore .",
    "lint": "cd web && concurrently --raw \"prettier --check --ignore-path ../.gitignore .\" \"eslint --ignore-path ../.gitignore .\""
  },
  "dependencies": {
    "@blueprintjs/core": "^4.8.0",
    "@blueprintjs/eslint-plugin": "^2.2.0",
    "@blueprintjs/icons": "^4.4.1",
    "@blueprintjs/popover2": "^1.5.1",
    "@blueprintjs/select": "^4.5.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/eslint": "^8",
    "@types/glob": "^7.2.0",
    "@types/lodash.debounce": "^4",
    "@types/node": "^18.7.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-refresh": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "can-autoplay": "^3.0.2",
    "chatsounds-web": "./pkg",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "^6.1.0",
    "glob": "^8.0.3",
    "lodash.debounce": "^4.0.8",
    "normalize.css": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.7",
    "react-use": "^17.4.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "resolutions": {
    "chatsounds-web": "portal:pkg"
  },
  "devDependencies": {
    "@types/can-autoplay": "^3"
  }
}
