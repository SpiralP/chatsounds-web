{
  "name": "chatsounds-web-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cd web && ts-node-esm discord-embed-proxy.ts",
    "dev": "npm run build:js dev",
    "build": "cd web && cross-env NODE_ENV=production npm run build:js",
    "build:dev": "cd web && npm run build:js",
    "build:js": "rimraf dist && cd web && ts-node-esm build.ts",
    "build:wasm": "wasm-pack build --target web",
    "typecheck": "cd web && tsc --noEmit",
    "format": "cd web && prettier --write --ignore-path ../.gitignore .",
    "lint": "cd web && concurrently --raw \"prettier --check --ignore-path ../.gitignore .\" \"eslint --ignore-path ../.gitignore .\""
  },
  "resolutions": {
    "chatsounds-web": "portal:pkg"
  },
  "dependencies": {
    "@types/async-lock": "^1.3.0",
    "@types/concat-stream": "^2.0.0",
    "@types/express": "^4.17.15",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.11.18",
    "async-lock": "^1.4.0",
    "concat-stream": "^2.0.0",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "memoizee": "^0.4.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@blueprintjs/core": "^4.14.1",
    "@blueprintjs/eslint-plugin": "^2.6.1",
    "@blueprintjs/icons": "^4.12.1",
    "@blueprintjs/popover2": "^1.11.1",
    "@blueprintjs/select": "^4.8.14",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/can-autoplay": "^3.0.1",
    "@types/eslint": "^8.4.10",
    "@types/glob": "^8.0.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-refresh": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "can-autoplay": "^3.0.2",
    "chatsounds-web": "./pkg",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "^6.1.0",
    "glob": "^8.1.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash.debounce": "^4.0.8",
    "normalize.css": "^8.0.1",
    "prettier": "^2.8.3",
    "react": "^17",
    "react-dom": "^17",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.7",
    "react-use": "^17.4.0",
    "rimraf": "^4.0.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslint": "^6.1.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
